{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/productos",
        "destination": "/productos.html"
      },
      {
        "source": "/contacto",
        "destination": "/contacto.html"
      },
      {
        "source": "/nosotros",
        "destination": "/nosotros.html"
      }
    ]
  }
}